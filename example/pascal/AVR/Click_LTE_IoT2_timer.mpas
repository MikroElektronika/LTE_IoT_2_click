unit Click_LTE_IoT2_timer;

uses Click_LTE_IoT2_types;

procedure lteiot2_configTimer(); 

implementation

procedure lteiot2_configTimer(); 
begin

    SREG_I_bit := 1; 
    TCCR1A := 0x80; 
    TCCR1B := 0x09; 
    OCR1AH := 0x1F; 
    OCR1AL := 0x3F; 
    OCIE1A_bit := 1; 

end;

procedure Timer_interrupt(); org IVT_ADDR_TIMER1_COMPA;
begin

    lteiot2_tick(); 

end;

end.